<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>
		<!-- report theme --> <!-- standard, dark -->
		<theme>dark</theme>

		<!-- document encoding -->  <!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->   <!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle>DSAlgoAvengers</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>DSAlgoAvengers - Cucumber Extent Report</reportName>

		<!-- global date format override -->  <!--
		defaults to yyyy-MM-dd -->
		<dateFormat>yyyy-MM-dd</dateFormat>

		<!-- global time format override -->   <!-- defaults to HH:mm:ss -->
		<timeFormat>HH:mm:ss</timeFormat>

		<!-- custom javascript -->
		<scripts>
      <![CDATA[
        $(document).ready(function() {
        
        });
      ]]>
    </scripts>

		<!-- custom styles -->
		<!-- Add this inside the <styles> section in configs.xml -->
		<styles>
		   <![CDATA[
		     body {
		         font-family: Arial, sans-serif; /* Use a default font */
		     }
		   ]]>
		</styles>

	</configuration>
</extentreports>